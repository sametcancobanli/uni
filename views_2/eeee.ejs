              <% for(var i=0; i<all_post.length; i++) {%>
                  <div class="card mb-2">
                      <div class="card-body p-2 p-sm-3">
                          <div class="media forum-item">
                              <a href="#" data-toggle="collapse" data-target=".forum-content"><img src="https://bootdey.com/img/Content/avatar/avatar7.png" class="mr-3 rounded-circle" width="50" alt="User" /></a>
                              <div class="media-body">
                                  <h6><a  href="/profile/<%= all_post[i]["user.user_id"] %>" class="nav-link"><%= all_post[i]["user.name"] %> <%= all_post[i]["user.surname"] %></a></h6>
                                  <p class="text-secondary">
                                      <%= all_post[i].p_text %>
                                  </p>
